{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "build": {
      "dependsOn": [
        "test",
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
      "cache": false
    }
  }
}
